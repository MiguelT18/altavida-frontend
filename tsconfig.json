{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      //? public
      "@public/*": [
        "public/*"
      ],
      //? src
      "@src/*": [
        "src/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@lib/*": [
        "src/lib/*"
      ],
      "@layout/*": [
        "src/layouts/*"
      ],
      "@content/*": [
        "src/content/*"
      ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  },
  "extends": "astro/tsconfigs/strict",
  "plugins": {
    "postcss-import": {},
    "tailwindcss/nesting": "postcss-nesting",
    "tailwindcss": {},
    "autoprefixer": {},
  }
}